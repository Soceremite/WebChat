<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title</title>

    <link href=@Url.Content("~/Content/css/reset.css") rel="stylesheet" />
    <link href=@Url.Content("~/Content/bootstrap.min.css") rel="stylesheet" />
    <link href=@Url.Content("~/Content/css/HiChat.css") rel="stylesheet" />
    <!-- plugins:css -->
    <link href=@Url.Content("~/vendors/iconfonts/mdi/css/materialdesignicons.min.css") rel="stylesheet">
    <link href=@Url.Content("~/vendors/iconfonts/puse-icons-feather/feather.css") rel="stylesheet">
    <link href=@Url.Content("~/vendors/css/vendor.bundle.base.css") rel="stylesheet">
    <link href=@Url.Content("~/vendors/css/vendor.bundle.addons.css") rel="stylesheet">
    <!-- endinject -->
    <link rel="icon" href="1111.ico" type="image/vnd.microsoft.icon" />
    <link rel="shortcut icon" href="/Resource/web/logo.ico" type="image/x-icon" />
    <!-- inject:css -->
    <link href=@Url.Content("/Content/css/style.css") rel="stylesheet">
    <link href=@Url.Content("/Content/css/my.css") rel="stylesheet">
    <!-- endinject -->
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- plugins:js -->
    <script src=@Url.Content("/vendors/js/vendor.bundle.base.js")></script>
    <script src=@Url.Content("/vendors/js/vendor.bundle.addons.js")></script>
    <!-- endinject -->
    <!-- inject:js -->
    <script src=@Url.Content("/Scripts/menu/off-canvas.js")></script>
    <script src=@Url.Content("/Scripts/menu/hoverable-collapse.js")></script>
    <script src=@Url.Content("/Scripts/menu/misc.js")></script>
    <script src=@Url.Content("/Scripts/menu/settings.js")></script>
    <script src=@Url.Content("/Scripts/menu/todolist.js")></script>
    <!-- endinject -->
    <!-- Custom js for this page-->
    <script src=@Url.Content("~/Scripts/jquery-3.4.1.min.js")></script>
    <script src=@Url.Content("~/Scripts/bootstrap.min.js")></script>
    <script src=@Url.Content("~/Scripts/jquery.signalR-2.4.2.min.js")></script>
    <script src="~/signalr/hubs"></script>
</head>
<body>
    @RenderBody()
    <!-- Include all compiled plugins (below), or include individual files as needed -->

    @RenderSection("scripts", required: false)
</body>
</html>

